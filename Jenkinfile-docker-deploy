
 pipeline {    agent any

       stages {
       stage('Preparation') {
           steps {
            git 'https://github.com/RamachandraAnnadi/ofbiz-demo.git'
         }
   stage('Sonarqube') {
    environment {
        scannerHome = tool 'SonarQubeScanner'
    }
    steps {
        withSonarQubeEnv('sonarqube') {
            sh "${scannerHome}/bin/sonar-scanner"
        }
           }
        }
     }
}
}
